extends ../layout

block content
    section#main-content
      section.wrapper
            include ../../partials/flash
            .row
                .col-lg-9.main-chart
                    // CUSTOM CHART START
                    .border-head
                        h3 ANNUAL CASE HEARING
                    //here is graph for hearing cases
                    .custom-bar-chart
                        ul.y-axis
                            li
                              span 10
                            li
                              span 8
                            li
                              span 6
                            li
                             span 4
                            li
                             span 2
                            li
                               span 0
                        .bar
                            .title JAN
                            .value.tooltips(data-original-title='8.500', data-toggle='tooltip', data-placement='top') 85%
                        .bar
                            .title FEB
                            .value.tooltips(data-original-title='5.000', data-toggle='tooltip', data-placement='top') 50%
                        .bar
                            .title MAR
                            .value.tooltips(data-original-title='6.000', data-toggle='tooltip', data-placement='top') 60%
                        .bar
                            .title APR
                            .value.tooltips(data-original-title='4.500', data-toggle='tooltip', data-placement='top') 45%
                        .bar
                            .title MAY
                            .value.tooltips(data-original-title='3.200', data-toggle='tooltip', data-placement='top') 32%
                        .bar
                            .title JUN
                            .value.tooltips(data-original-title='6.200', data-toggle='tooltip', data-placement='top') 62%
                        .bar
                            .title JUL
                            .value.tooltips(data-original-title='7.500', data-toggle='tooltip', data-placement='top') 75%
                    .row.mt
                      .col-md-4.col-sm-4.mb
                        .grey-panel.pn.donut-chart
                            .grey-header
                                  h5 CASES LISTENING IN A DAY FROM ALL
                            canvas#serverstatus01(height='120', width='120')
                            script.
                                var doughnutData = [{
                                value: 70,
                                color: "#FF6B6B"
                                },
                                {
                                value: 30,
                                color: "#fdfdfd"
                                }
                                ];
                                var myDoughnut = new Chart(document.getElementById("serverstatus01").getContext("2d")).Doughnut(doughnutData);
                            .row
                            .col-sm-6.col-xs-6.goleft
                                p
                                | Usage
                                br
                                | Increase:
                            .col-sm-6.col-xs-6
                                h2 21%
                        
                      .col-md-4.col-sm-4.mb
                         .grey-panel.pn.donut-chart
                            .grey-header
                                  h5 CASE LISTENNED IN A DAY FROM A DAY SCHEDULE
                            canvas#serverstatus02(height='120', width='120')
                            script.
                                var doughnutData = [{
                                value: 90,
                                color: "#FF6B6B"
                                },
                                {
                                value: 10,
                                color: "#fdfdfd"
                                }
                                ];
                                var myDoughnut = new Chart(document.getElementById("serverstatus02").getContext("2d")).Doughnut(doughnutData);
                            .row
                            .col-sm-6.col-xs-6.goleft
                                p
                                | Usage
                                br
                                | Increase:
                            .col-sm-6.col-xs-6
                                h2 90%
                      .col-md-4.col-sm-4.mb
                         .grey-panel.pn.donut-chart
                            .grey-header
                                  h5 POSTPONDED CASE LISTENNED IN A DAY
                            canvas#serverstatus03(height='120', width='120')
                            script.
                                var doughnutData = [{
                                value: 60,
                                color: "#FF6B6A"
                                },
                                {
                                value: 40,
                                color: "#fdfdfd"
                                }
                                ];
                                var myDoughnut = new Chart(document.getElementById("serverstatus03").getContext("2d")).Doughnut(doughnutData);
                            .row
                            .col-sm-6.col-xs-6.goleft
                                p
                                | Usage
                                br
                                | Increase:
                            .col-sm-6.col-xs-6
                                h2 60%                                
                .col-lg-3.ds
                    h4.centered.mt CURRENT ONGOING CASES IN ROOMS
                    .desc
                        .thumb
                          span.badge.bg-theme
                            i.fa.fa-clock-o
                        .details
                          p
                            muted A1233 
                            br
                            a murdercase
                            br
